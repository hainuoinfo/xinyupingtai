  `state` int(2) NOT NULL DEFAULT '0',
  PRIMARY KEY (`uid`),
  KEY `i1` (`vip`,`vip_expire`,`uid`),
  KEY `i2` (`black2`,`uid`),
  KEY `i3` (`credits`,`uid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

